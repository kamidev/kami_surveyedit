{
  "locale": "sv",
  "title": "Projektuppgifter",
  "pages": [
    {
      "name": "administrative",
      "elements": [
        {
          "type": "text",
          "name": "project_title",
          "title": {
            "sv": "Projektnamn"
          },
          "isRequired": true
        },
        {
          "type": "text",
          "name": "project_id",
          "title": {
            "sv": "Diarienummer"
          },
          "isRequired": true
        },
        {
          "type": "comment",
          "name": "project_description",
          "title": {
            "sv": "Beskrivning"
          },
          "isRequired": true,
          "maxLength": 600,
          "rows": 2,
          "placeHolder": {
            "sv": "Max 600 tecken inkl blanksteg. Skriv gärna med tanke på en bredare publik."
          }
        },
        {
          "type": "text",
          "name": "project_link",
          "title": {
            "sv": "Webbplats"
          },
          "isRequired": true,
          "inputType": "url"
        }
      ],
      "title": {
        "sv": "Administrativt"
      }
    },
    {
      "name": "project_info",
      "elements": [
        {
          "type": "text",
          "name": "project_organization",
          "title": {
            "sv": "Organisation"
          },
          "isRequired": true
        },
        {
          "type": "dropdown",
          "name": "project_type",
          "title": {
            "sv": "Projekttyp"
          },
          "isRequired": true,
          "choices": [
            {
              "value": "study",
              "text": {
                "sv": "Förstudie"
              }
            },
            {
              "value": "research",
              "text": {
                "sv": "Forskningsprojekt"
              }
            },
            {
              "value": "innovation",
              "text": {
                "sv": "Innovationsprojekt"
              }
            },
            {
              "value": "demo",
              "text": {
                "sv": "Demonstrationsprojekt"
              }
            }
          ]
        },
        {
          "type": "text",
          "name": "project_start",
          "title": {
            "sv": "Projektstart"
          },
          "placeHolder": "YY-MM-DD"
        },
        {
          "type": "text",
          "name": "project_end",
          "title": {
            "sv": "Projektslut"
          },
          "placeHolder": "YY-MM-DD"
        },
        {
          "type": "comment",
          "name": "keywords",
          "title": {
            "sv": "Nyckelord"
          }
        }
      ],
      "title": {
        "sv": "Projektinformation"
      }
    },
    {
      "name": "funding",
      "elements": [
        {
          "type": "multipletext",
          "name": "budget",
          "startWithNewLine": false,
          "title": {
            "sv": "Budget"
          },
          "isRequired": true,
          "items": [
            {
              "name": "funded",
              "inputType": "number",
              "title": "Sökt stöd"
            },
            {
              "name": "total_cost",
              "inputType": "number",
              "title": {
                "sv": "Totalkostnad"
              }
            }
          ]
        },
        {
          "type": "expression",
          "name": "ratio_funded",
          "startWithNewLine": false,
          "title": "Andel sökt stöd:",
          "expression": "{budget.funded} / {budget.total_cost}",
          "displayStyle": "percent"
        },
        {
          "type": "comment",
          "name": "other_financiers",
          "title": {
            "sv": "Övriga finansiärer"
          }
        },
        {
          "type": "comment",
          "name": "other_recipients",
          "title": {
            "sv": "Övriga mottagare"
          }
        }
      ],
      "title": {
        "sv": "Finansiering"
      }
    },
    {
      "name": "theme_focus_matrix",
      "elements": [
        {
